{
	"roles": {
		"managed": [{
				"roleName": "ReadOnly",
				"policyArn": "arn:aws:iam::aws:policy/ReadOnlyAccess",
				"federate": true
			},
			{
				"roleName": "UserAdmin",
				"policyArn": "arn:aws:iam::aws:policy/IAMFullAccess",
				"federate": true
			},
			{
				"roleName": "SysEng",
				"policyArn": "arn:aws:iam::aws:policy/PowerUserAccess",
				"federate": true
			},
			{
				"roleName": "DBAdmin",
				"policyArn": "arn:aws:iam::aws:policy/job-function/DatabaseAdministrator",
				"federate": true
			},
			{
				"roleName": "FullAdmin",
				"policyArn": "arn:aws:iam::aws:policy/AdministratorAccess",
				"federate": true
			},
			{
				"roleName": "DatadogAWSIntegrationRole",
				"federate": false
			}, 
			{
				"roleName": "NetworkAdmin",
				"policyArn": "arn:aws:iam::aws:policy/job-function/NetworkAdministrator",
				"federate": true
			},
			{
				"roleName": "PowerUser",
				"federate": true
			}
		],
		"self-managed": [{
				"roleName": "ReadOnly",
				"policyArn": "arn:aws:iam::aws:policy/ReadOnlyAccess",
				"federate": true
			},
			{
				"roleName": "UserAdmin",
				"policyArn": "arn:aws:iam::aws:policy/IAMFullAccess",
				"federate": true
			},
			{
				"roleName": "SysEng",
				"policyArn": "arn:aws:iam::aws:policy/PowerUserAccess",
				"federate": true
			},
			{
				"roleName": "DBAdmin",
				"policyArn": "arn:aws:iam::aws:policy/job-function/DatabaseAdministrator",
				"federate": true
			},
			{
				"roleName": "FullAdmin",
				"policyArn": "arn:aws:iam::aws:policy/AdministratorAccess",
				"federate": true
			}
		],
		"craws": [{
				"roleName": "FullAdmin",
				"policyArn": "arn:aws:iam::aws:policy/AdministratorAccess",
				"federate": true
			}
		]
	},
	"adminPolicyArn": "arn:aws:iam::aws:policy/AdministratorAccess",
	"scAdminPolicyArn": "arn:aws:iam::aws:policy/AWSServiceCatalogAdminFullAccess",
        "adminRolePath":"/",
	"assumeRolePolicyDocument": {
		"Version": "2012-10-17",
		"Statement": [{
			"Sid": "",
			"Effect": "Allow",
			"Principal": {
				"AWS": null
			},
			"Action": "sts:AssumeRole",
			"Condition": {
				"StringEquals": {
					"sts:ExternalId": null
				}
			}
		}]
	},
	"PowerUserPolicyDocument":{
		"Version": "2012-10-17",
  		"Statement": [
    		  {
            		"Effect": "Allow",
            		"Action": [
                		"ec2:DescribeInstanceAttribute",
                		"ec2:DescribeInstanceStatus",
                		"ec2:DescribeInstances"
			],
            		"Resource": "*"
    		  },
    		  {
      			"Action": [
        			"ec2:StartInstances",
        			"ec2:StopInstances",
				"ec2:RebootInstances"
      			],
      			"Effect": "Allow",
      			"Resource": "arn:aws:ec2:*:*:instance/*"
    		  }		
  		]
	},
    "createUserIAMPolicyDocument":{
        "Version": "2012-10-17",
        "Statement": [
        {
         "Sid": "SCUserCreationPolicy",
         "Effect": "Allow",
         "Action": [
            "iam:CreatePolicy",
            "iam:AttachUserPolicy",
            "iam:CreateUser",
            "iam:CreateAccessKey"
         ],
         "Resource": "*"
        }
        ]
    }
}
